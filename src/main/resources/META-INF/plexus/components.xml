<?xml version="1.0"?>
<component-set>
  <components>
    <component>
      <role>org.apache.maven.lifecycle.Lifecycle</role>
      <implementation>org.apache.maven.lifecycle.Lifecycle</implementation>
      <role-hint>avaje-metrics-maven-plugin</role-hint>
      <configuration>
        <id>avaje-metrics-maven-plugin</id>
        <phases>
          <phase>avaje-metrics-maven-not-used-phase</phase>
        </phases>
        <default-phases>
          <process-classes>
            io.avaje.metrics:metrics-maven-plugin:${project.version}:enhance
          </process-classes>
        </default-phases>
      </configuration>
    </component>
  </components>
</component-set>
